{
  "name": "ddwp-frontend",
  "version": "0.0.2",
  "private": true,
  "description": "A mini-program for baidu competition",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "author": "Colin Wang",
  "contributors": [
    {
      "name": "Colin Wang",
      "email": "qingyu.wang@aliyun.com"
    }
  ],
  "lint-staged": {
    "*.{ts,tsx}": "yarn lint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn build:swan",
      "pre-push": "echo $HUSKY_GIT_STDIN | scripts/hooks/pre-push-lfs.sh $HUSKY_GIT_PARAMS",
      "post-checkout": "echo $HUSKY_GIT_STDIN | scripts/hooks/post-checkout-lfs.sh $HUSKY_GIT_PARAMS",
      "post-commit": "echo $HUSKY_GIT_STDIN | scripts/hooks/post-commit-lfs.sh $HUSKY_GIT_PARAMS",
      "post-merge": "echo $HUSKY_GIT_STDIN | scripts/hooks/post-merge-lfs.sh $HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "dump": "apollo client:download-schema",
    "graphql:dump": "yarn dump schema.graphql",
    "gen": "apollo client:codegen --target typescript",
    "lint": "eslint --fix .",
    "build": "yarn build:swan",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "start": "yarn dev:swan",
    "dev:weapp": "yarn build:weapp --watch",
    "dev:swan": "yarn build:swan --watch",
    "dev:alipay": "yarn build:alipay --watch",
    "dev:tt": "yarn build:tt --watch",
    "dev:h5": "yarn build:h5 --watch",
    "dev:rn": "yarn build:rn --watch",
    "dev:qq": "yarn build:qq --watch",
    "dev:jd": "yarn build:jd --watch",
    "dev:quickapp": "yarn build:quickapp --watch"
  },
  "license": "MIT",
  "dependencies": {
    "@tarojs/cli": "2.2.15",
    "@tarojs/components": "2.2.15",
    "@tarojs/components-qa": "2.2.15",
    "@tarojs/router": "2.2.15",
    "@tarojs/taro": "2.2.15",
    "@tarojs/taro-alipay": "2.2.15",
    "@tarojs/taro-h5": "2.2.15",
    "@tarojs/taro-qq": "2.2.15",
    "@tarojs/taro-quickapp": "2.2.15",
    "@tarojs/taro-swan": "2.2.15",
    "@tarojs/taro-tt": "2.2.15",
    "@tarojs/taro-weapp": "2.2.15",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "graphql": "^14.0.0",
    "nerv-devtools": "^1.5.5",
    "nervjs": "^1.5.5",
    "react": "^17.0.0",
    "regenerator-runtime": "0.11.1",
    "subscriptions-transport-ws": "^0.9.18",
    "taro-apollo-client": "2.2.17-0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@tarojs/mini-runner": "2.2.15",
    "@tarojs/plugin-less": "2.2.15",
    "@tarojs/plugin-terser": "^2.2.10",
    "@tarojs/webpack-runner": "2.2.15",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.53",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "apollo": "^2.31.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-taro": "2.2.15",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-taro": "2.2.15",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "stylelint": "13.7.2",
    "stylelint-config-taro-rn": "2.2.15",
    "stylelint-taro-rn": "2.2.15",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
